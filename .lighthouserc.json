{
  "ci": {
    "collect": {
      "startServerCommand": "npm run start", 
      "url": [
        "http://localhost:44711/",
        "http://localhost:44711/blogs/1",
        "http://nikomehilainen.com/",
        "http://nikomehilainen.com/blogs/1"
      ],
      "numberOfRuns": 3,
      "settings": {
        "chromeFlags": "--no-sandbox --disable-dev-shm-usage --headless"
      }
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "csp-xss": "off",
        "installable-manifest": "off",
        "splash-screen": "off",
        "themed-omnibox": "off",
        "maskable-icon": "off",
        "total-byte-weight": "warn",
        "unused-javascript": "warn"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
